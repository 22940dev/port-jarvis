{
  "presets": [
    [
      "@babel/preset-env",
      {
        "include": [
          "transform-arrow-functions",
          "transform-block-scoping",
          "transform-template-literals"
        ],
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    [
      "minify",
      {
        "removeConsole": true,
        // https://github.com/babel/minify/issues/904#issuecomment-424367995
        "builtIns": false
      }
    ]
  ]
}
